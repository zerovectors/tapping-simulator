	print("Reason: "..message)
	hintmsg.Text = "Initializing Failed, Error; "..message
	if message == "HttpError: ConnectFail" then
		hintmsg.Text = "Initializing Failed!| Error; "..message.." | Fix; Re-Execute or ReJoin"
	end
	wait(6)
	hintmsg:Remove()
end)
